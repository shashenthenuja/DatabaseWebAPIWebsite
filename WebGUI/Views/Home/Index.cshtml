@{
   @using DataAccessLayer.Models;
   @model List<BankData>;
}
<div id="tableview">
    <table>

        @foreach(var data in Model)
        {
            <tr>
                <td>@data.Id</td>
                <td>@data.FirstName</td>
                <td>@data.LastName</td>
                <td>@data.AccNum</td>
                <td>@data.Pin</td>
                <td>@data.Balance</td>
            </tr>
        }
    </table>
</div>